<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.heye.crm.server.dao.HyProductMapper">
    <resultMap id="BaseResultMap" type="com.heye.crm.common.model.HyProduct">
        <id column="pro_id" property="proId"/>
        <result column="pro_name" property="proName"/>
        <result column="pro_model" property="proModel"/>
        <result column="pro_desc" property="proDesc"/>
        <result column="pro_pictures" property="proPictures"/>
        <result column="pro_bar_code" property="proBarCode"/>
        <result column="pro_price" property="proPrice"/>
        <result column="pro_state" property="proState"/>
        <result column="pro_display" property="proDisplay"/>
        <result column="pro_unit_price" property="proUnitPrice"/>
        <result column="pro_type" property="proType"/>
        <result column="created_at" property="createdAt"/>
        <result column="updated_at" property="updateAt"/>
    </resultMap>

    <sql id="Table">hr_product</sql>
</mapper>